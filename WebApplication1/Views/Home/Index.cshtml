@model List<Library.User>
@{
    ViewBag.Title = "Все пользователи";
}
<a asp-action="Create">Добавить пользователя</a>
@*<a asp-action="Edit">Редактировать пользователя</a>*@
<a asp-action="Delete">Удалить пользователя</a>

<table class="table">
    <tr><td>Id</td><td>Имя</td><td>Возраст</td></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Age</td>
        </tr>
    }
</table>

